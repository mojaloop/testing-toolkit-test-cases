          "scripts": {
            "postRequest": {
              "exec": [
                "\r",
                "var jsonData = response.body;\r",
                "console.log(jsonData);\r",
                "    for(var i in jsonData) {\r",
                "        if((jsonData[i].ledgerAccountType === 'SETTLEMENT') && (jsonData[i].currency === environment.currency) ) {\r",
