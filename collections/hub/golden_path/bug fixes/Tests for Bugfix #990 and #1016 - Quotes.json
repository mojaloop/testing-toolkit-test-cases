      "id": 1,
      "name": "Tests for Bugfix #990 and #1016 - Quotes",
      "requests": [
        {
          "id": 3,
          "description": "Send quote",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "fspiop",
            "asynchronous": true
          },
          "operationPath": "/quotes",
          "method": "post",
