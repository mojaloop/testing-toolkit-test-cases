        {
          "id": "Add-e2eSim3",
          "meta": {
            "info": "Add e2eSim3"
          },
          "description": "Add e2eSim3",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "central_admin"
          },
          "operationPath": "/participants",
          "path": "/participants",
          "method": "post",
          "body": {
            "name": "{$inputs.E2ESIM3_NAME}",
            "currency": "{$inputs.currency3}"
          },
          "headers": {
            "Content-Type": "application/json",
            "FSPIOP-Source": "{$inputs.hub_operator}"
          },
          "url": "{$inputs.HOST_CENTRAL_LEDGER}",
          "tests": {
            "assertions": [
              {
                "id": "status-201-if-not-exists-400-i",
                "description": "status to be 201 if not exists or 400 if exists",
                "exec": [
                  "if (response.body.errorInformation) {",
                  "  expect(response.status).to.equal(400)",
                  "} else {",
                  "  expect(response.status).to.equal(201)",
                  "}"
                ]
              }
            ]
          }
        },
        {
          "id": "Add-initial-pos-limits-e2eSim3",
          "meta": {
            "info": "Add initial position and limits - e2eSim3"
          },
          "description": "Add initial position and limits - e2eSim3",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "central_admin"
          },
          "operationPath": "/participants/{name}/initialPositionAndLimits",
          "path": "/participants/{$inputs.E2ESIM3_NAME}/initialPositionAndLimits",
          "method": "post",
          "params": {
            "name": "{$inputs.E2ESIM3_NAME}"
          },
          "body": {
            "currency": "{$inputs.currency3}",
            "limit": {
              "type": "NET_DEBIT_CAP",
              "value": 500000
            },
            "initialPosition": 0
          },
          "headers": {
            "Content-Type": "application/json",
            "FSPIOP-Source": "{$inputs.hub_operator}"
          },
          "url": "{$inputs.HOST_CENTRAL_LEDGER}",
          "tests": {
            "assertions": [
              {
                "id": "status-201-if-not-exists-500-i",
                "description": "status to be 201 if not exists or 500 if exists",
                "exec": [
                  "if (response.body.errorInformation) {",
                  "  expect(response.status).to.equal(500)",
                  "} else {",
                  "  expect(response.status).to.equal(201)",
                  "}"
                ]
              }
            ]
          }
        },
        {
          "id": "e2eSim3-Get-Status-req-before-",
          "meta": {
            "info": "e2eSim3 Get Status Request before deposit"
          },
          "description": "e2eSim3 Get Status Request before deposit",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "central_admin"
          },
          "operationPath": "/participants/{name}/accounts",
          "path": "/participants/{$inputs.E2ESIM3_NAME}/accounts",
          "method": "get",
          "params": {
            "name": "{$inputs.E2ESIM3_NAME}"
          },
          "headers": {
            "Content-Type": "application/json",
            "FSPIOP-Source": "{$inputs.hub_operator}"
          },
          "url": "{$inputs.HOST_CENTRAL_LEDGER}",
          "tests": {
            "assertions": [
              {
                "id": "status-200",
                "description": "status to be 200",
                "exec": [
                  "expect(response.status).to.equal(200)"
                ]
              }
            ]
          },
          "scripts": {
            "postRequest": {
              "exec": [
                "const jsonData = pm.response.body",
                "console.log(jsonData)",
                "for(var i in jsonData) {",
                "  if(jsonData[i].ledgerAccountType === 'SETTLEMENT' && jsonData[i].currency === pm.environment.get('currency3')) {",
                "    pm.environment.set(\"e2eSim3SettlementAccountId\", jsonData[i].id)",
                "    pm.environment.set(\"e2eSim3SettlementAccountBalanceBeforeFundsIn\", jsonData[i].value)",
                "  }",
                "}"
              ]
            }
          }
        },
        {
          "id": "Deposit-Funds-settle-acc-ttksi",
          "meta": {
            "info": "Deposit Funds in Settlement Account - e2eSim3"
          },
          "description": "Deposit Funds in Settlement Account - e2eSim3",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "central_admin"
          },
          "operationPath": "/participants/{name}/accounts/{id}",
          "path": "/participants/{$inputs.E2ESIM3_NAME}/accounts/{$environment.e2eSim3SettlementAccountId}",
          "method": "post",
          "params": {
            "name": "{$inputs.E2ESIM3_NAME}",
            "id": "{$environment.e2eSim3SettlementAccountId}"
          },
          "body": {
            "transferId": "{$environment.e2eSim3fundsInPrepareTransferId}",
            "externalReference": "string",
            "action": "recordFundsIn",
            "reason": "string",
            "amount": {
              "amount": "{$environment.e2eSim3fundsInPrepareAmount}",
              "currency": "{$inputs.currency3}"
            },
            "extensionList": {
              "extension": [
                {
                  "key": "string",
                  "value": "string"
                }
              ]
            }
          },
          "headers": {
            "Content-Type": "application/json",
            "FSPIOP-Source": "{$inputs.hub_operator}"
          },
          "url": "{$inputs.HOST_CENTRAL_LEDGER}",
          "tests": {
            "assertions": [
              {
                "id": "status-202",
                "description": "status to be 202",
                "exec": [
                  "expect(response.status).to.equal(202)"
                ]
              }
            ]
          },
          "scripts": {
            "preRequest": {
              "exec": [
                "var uuid = require('uuid');",
                "pm.environment.set('e2eSim3fundsInPrepareTransferId', uuid.v4())",
                "pm.environment.set('e2eSim3fundsInPrepareAmount', 500000);"
              ]
            }
          }
        },
        {
          "id": "e2eSim3-Get-Status-req-after-d",
          "meta": {
            "info": "e2eSim3 Get Status Request after deposit"
          },
          "description": "e2eSim3 Get Status Request after deposit",
          "apiVersion": {
            "minorVersion": 0,
            "majorVersion": 1,
            "type": "central_admin"
          },
          "operationPath": "/participants/{name}/accounts",
          "path": "/participants/{$inputs.E2ESIM3_NAME}/accounts",
          "method": "get",
          "params": {
            "name": "{$inputs.E2ESIM3_NAME}"
          },
          "headers": {
            "Content-Type": "application/json",
            "FSPIOP-Source": "{$inputs.hub_operator}"
          },
          "url": "{$inputs.HOST_CENTRAL_LEDGER}",
          "tests": {
            "assertions": [
              {
                "id": "status-200",
                "description": "status to be 200",
                "exec": [
                  "expect(response.status).to.equal(200)"
                ]
              },
              {
                "id": "e2eSim3-settle-acc-bal-After-F",
                "description": "e2eSim3 Settlement Account Balance After FundsIn should be equal to the balance before plus the transfer amount",
                "exec": [
                  "expect(+environment.e2eSim3SettlementAccountBalanceAfterFundsIn).to.equal(+environment.e2eSim3SettlementAccountBalanceBeforeFundsIn - +environment.e2eSim3fundsInPrepareAmount)"
                ]
              }
            ]
          },
          "delay": "500",
          "scripts": {
            "postRequest": {
              "exec": [
                "const jsonData = pm.response.body",
                "console.log(jsonData)",
                "for(var i in jsonData) {",
                "  if(jsonData[i].ledgerAccountType === 'SETTLEMENT' && jsonData[i].currency === pm.environment.get('currency3')) {",
                "    pm.environment.set(\"e2eSim3SettlementAccountBalanceAfterFundsIn\", jsonData[i].value)",
                "  }",
                "}"
              ]
            }
          }
        }
      ]
